name: Page
tags:
  # Static content - rendered at build time
  - tag: slowlyRendered
    type: data
    dataType: string
    phase: slow
  
  # Dynamic content - rendered per request
  - tag: fastRendered
    type: data
    dataType: string
    phase: fast
  
  - tag: fastDynamicRendered
    type: data
    dataType: string
    phase: fast+interactive
  
  # Interactive element
  - tag: button
    type: interactive
    elementType: HTMLButtonElement
