<html>
<head>
    <script type="application/jay-headfull" src="./counter" names="Counter"></script>
    <script type="application/jay-data">
data:
  nestedCounters:
    - counter: number
      id: string
  condition: boolean
  count1: number
    </script>
</head>
<body>
<div>
    <jay:Counter forEach="nestedCounters" trackBy="id" ref="counter1" initialValue={counter} />
    <jay:Counter if="condition" ref="counter2" initialValue="{count1}" />
</div>
</body>
</html>