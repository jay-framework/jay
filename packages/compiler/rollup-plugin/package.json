{
  "name": "rollup-plugin-jay",
  "version": "1.0.0",
  "main": "dist/index.js",
  "keywords": [
    "jay",
    "secure",
    "rollup-plugin"
  ],
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:watch": "npm run build:js -- --watch & npm run build:types -- --watch",
    "build:watch:sandbox-counter": "vite -c test/jayRuntime/fixtures/counter/source/vite.config.ts",
    "build:js": "vite build",
    "build:types": "tsup lib/index.ts --dts-only --format cjs",
    "build:check-types": "tsc",
    "build:executable-bin": "chmod +x dist/index.js",
    "clean": "rimraf dist && rimraf test/jayRuntime/fixtures/counter/dist && rimraf test/jayRuntime/fixtures/exec/dist",
    "confirm": "npm run clean && npm run build && npm run build:check-types && npm run test",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "fast-glob": "^3.3.2",
    "jay-compiler": "workspace:^",
    "jay-compiler-jay-html": "workspace:^",
    "typescript": "^5.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "jay-component": "workspace:^",
    "jay-dev-environment": "workspace:^",
    "jay-runtime": "workspace:^",
    "jay-secure": "workspace:^",
    "rimraf": "^5.0.5",
    "rollup": "^4.9.5",
    "rollup-plugin-typescript2": "^0.36.0",
    "tsup": "^8.0.1",
    "vite": "^5.0.11",
    "vitest": "^1.2.1",
    "vitest-mock-extended": "^1.3.1"
  }
}
