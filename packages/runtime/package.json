{
  "name": "jay-runtime",
  "version": "1.0.0",
  "type": "module",
  "main": "dist/index",
  "scripts": {
    "build": "npm run build:js && npm run build:types && npm run build:fix-dts",
    "build:js": "vite build",
    "build:types": "tsup lib/index.ts --dts-only --format esm",
    "build:check-types": "tsc",
    "build:fix-dts": "node ./scripts/fix-dts.js",
    "clean": "rm -rf dist",
    "confirm": "npm run clean && npm run build && npm run build:check-types && npm run test",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "jay-list-compare": "workspace:^",
    "jay-reactive": "workspace:^",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.1.4",
    "@types/jsdom": "^21.1.5",
    "@types/node": "^20.9.2",
    "jay-dev-environment": "workspace:^",
    "replace-in-file": "^7.0.2",
    "tsup": "^8.0.0",
    "typescript": "^5.2.2",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
