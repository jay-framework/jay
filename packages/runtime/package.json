      "./jest-setup.js"
    ]
  },
  "dependencies": {},
  "devDependencies": {
    "typescript": "^4.0.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@rollup/plugin-typescript": "^8.1.0",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jsdom": "^16.2.4",
    "@types/node": "^14.11.10",
    "jest": "^26.6.0",
    "jsdom": "^16.4.0",
    "rollup": "^2.35.1",
    "tslib": "^2.0.3"
  }
}
