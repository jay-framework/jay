{
  "name": "jay-runtime",
  "version": "1.0.0",
  "main": "dist/index",
  "scripts": {
    "start": "tsc",
    "test": "jest",
    "rollup": "rollup --config ./rollup.config.js",
    "copy": "cp examples/index.html build/index.html && cp examples/index.css build/index.css",
    "build": "npm run rollup && npm run copy && echo 'build lib' && tsc && echo 'build examples' && tsc -p tsconfig-examples.json"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./jest-setup.js"
    ]
  },
  "dependencies": {
    "jay-reactive": "1.0.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@jest/types": "^28.1.3",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.3.4",
    "@testing-library/jest-dom": "^5.16.4",
    "@types/jsdom": "^16.2.15",
    "@types/node": "^14.18.23",
    "@types/testing-library__jest-dom": "^5.14.5",
    "jest": "^26.6.3",
    "jsdom": "^16.7.0",
    "rollup": "^2.77.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
