    "@babel/preset-env": "^7.14.2",
    "@babel/preset-typescript": "^7.13.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@testing-library/jest-dom": "^5.12.0",
    "@types/jsdom": "^16.2.10",
    "@types/node": "^14.17.0",
    "@types/testing-library__jest-dom": "^5.14.0",
    "caniuse-lite": "^1.0.30001282",
    "jest": "^26.6.3",
    "jsdom": "^16.5.3",
    "rollup": "^2.48.0",
    "ts-jest": "^26.5.6",
    "tslib": "^2.2.0",
    "typescript": "^4.2.4"
  }
