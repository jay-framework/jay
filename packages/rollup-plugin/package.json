{
  "name": "rollup-plugin-jay",
  "version": "1.0.0",
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:js": "vite build",
    "build:watch": "vite build --watch",
    "build:types": "tsup lib/index.ts --dts-only --format esm",
    "build:check-types": "tsc",
    "build:executable-bin": "chmod +x dist/index.js",
    "clean": "rm -rf dist",
    "confirm": "npm run clean && npm run build && npm run build:check-types && npm run test",
    "test": ":",
    "test:watch": ":"
  },
  "dependencies": {
    "jay-compiler": "workspace:^",
    "lodash": "^4.17.21",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@types/lodash": "^4",
    "@types/node": "^20.9.2",
    "jay-dev-environment": "workspace:^",
    "tsup": "^8.0.0",
    "vite": "^5.0.0",
    "vitest": "^0.34.6"
  }
}
