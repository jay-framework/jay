{
  "name": "@jay-framework/figma-interchange",
  "version": "0.0.1",
  "type": "module",
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "npm run build:js && npm run build:types",
    "build:watch": "npm run build:js -- --watch & npm run build:types -- --watch",
    "build:js": "vite build",
    "build:types": "tsup lib/index.ts --dts-only --format esm",
    "build:check-types": "tsc",
    "clean": "rimraf dist",
    "test": ":",
    "test:watch": ":"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.0"
  }
}
