<html>
<head>
    <script type="application/jay-headless" src="../../simple-plugin/simple-plugin.ts" name="plugin" key="plugin" ></script>
    <script type="application/jay-data">
data:
    pageSlowlyRendered: string
    pageFastDynamicRendered: string
    </script>
</head>
<body>
<div>
    <!-- Plugin values -->
    <h2>Plugin Values</h2>
    <div>Plugin Slow: {plugin.slowlyRendered}</div>
    <div>Plugin Fast: {plugin.fastDynamicRendered}</div>
    <button ref="plugin.pluginButton" data-id="plugin-button">Plugin Button</button>

    <!-- Page values -->
    <h2>Page Values</h2>
    <div>Page Slow: {pageSlowlyRendered}</div>
    <div>Page Fast: {pageFastDynamicRendered}</div>
    <button ref="button" data-id="page-button">Page Button</button>
</div>
</body>
</html> 