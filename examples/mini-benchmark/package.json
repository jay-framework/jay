{
  "name": "jay-example-mini-benchmark",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "scripts": {
    "d": "jay-cli definitions ./lib",
    "jay-secure": "jay-cli runtime ./lib ./lib-secure/main",
    "rollup": "echo 'rollup --config ./rollup.config.js'",
    "build": "npm run d && npm run mk-secure && npm run rollup",
    "confirm": "npm run clean && npm run build && npm run test",
    "copy": "cp ./lib/main.ts ./lib-secure/sandbox/ && cp ./lib/benchmark.ts ./lib-secure/sandbox/",
    "copy-basic": "mkdir -p ./lib-secure/sandbox/basic/ && cp ./lib/basic/*-refs.d.ts ./lib-secure/main/basic && cp ./lib/basic/basic-data.ts ./lib-secure/sandbox/basic/",
    "copy-collection": "mkdir -p ./lib-secure/sandbox/collections/ && cp ./lib/collections/*-refs.d.ts ./lib-secure/main/collections && cp ./lib/collections/collections-data.ts ./lib-secure/sandbox/collections/",
    "copy-composite": "mkdir -p ./lib-secure/sandbox/composite/ && cp ./lib/composite/*-refs.d.ts ./lib-secure/main/composite && cp ./lib/composite/composite-data.ts ./lib-secure/sandbox/composite/",
    "copy-condition": "mkdir -p ./lib-secure/sandbox/conditions/ && cp ./lib/conditions/*-refs.d.ts ./lib-secure/main/conditions && cp ./lib/conditions/conditions-data.ts ./lib-secure/sandbox/conditions/",
    "copy-table": "mkdir -p ./lib-secure/sandbox/table/ && cp ./lib/table/*-refs.d.ts ./lib-secure/main/table && cp ./lib/table/table-host.ts ./lib-secure/sandbox/table/ && cp ./lib/table/table.ts ./lib-secure/sandbox/table/",
    "mk-secure": "npm run jay-secure && npm run copy && npm run copy-basic && npm run copy-collection && npm run copy-composite && npm run copy-condition && npm run copy-table",
    "clean": "rm -rf dist && (rm -rf lib/**/*.d.ts || :)",
    "test": ":"
  },
  "dependencies": {
    "immer": "^10.0.3",
    "jay-component": "workspace:^",
    "jay-json-patch": "workspace:^",
    "jay-reactive": "workspace:^",
    "jay-runtime": "workspace:^",
    "jay-secure": "workspace:^",
    "jay-serialization": "workspace:^"
  },
  "devDependencies": {
    "@babel/core": "^7.23.3",
    "@babel/preset-env": "^7.23.3",
    "@babel/preset-typescript": "^7.23.3",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "jay-cli": "workspace:^",
    "rollup": "^4.5.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-jay": "workspace:^",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.36.0",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  }
}
