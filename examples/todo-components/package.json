{
  "name": "jay-example-todo-components",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "d": "node node_modules/jay-cli/dist/index.js definitions ./lib",
    "jay": "node node_modules/jay-cli/dist/index.js runtime ./lib",
    "rollup": "rollup --config ./rollup.config.js",
    "build": "npm run d && npm run rollup",
    "mk-secure": "npm run jay && mv ./lib/item.jay.html.ts ./lib-secure/main && mv ./lib/todo.jay.html.ts ./lib-secure/main && cp ./lib/item.ts ./lib-secure/sandbox && cp ./lib/todo.ts ./lib-secure/sandbox && cp ./lib/uuid.ts ./lib-secure/sandbox"
  },
  "dependencies": {
    "jay-component": "1.0.0",
    "jay-mutable": "1.0.0",
    "jay-runtime": "1.0.0",
    "jay-reactive": "1.0.4",
    "jay-secure": "1.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-typescript": "^7.18.6",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@rollup/plugin-typescript": "^8.3.4",
    "jay-cli": "1.0.0",
    "rollup": "^2.77.2",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-jay": "1.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4"
  }
}
