<html>
  <head>
    <script type="application/jay-data">
      data:
        products:
          - name: string
            price: number
            sku: string
            id: string
            slug: string
    </script>
  </head>
  <body>
    <div>
      products
      <ul>
        <li foreach="products" trackby="id">
          <a href="/products/{slug}">
            <div>{name}</div>
            <div>{price}</div>
            <div>{sku}</div>
          </a>
        </li>
      </ul>
    </div>
  </body>
</html>
