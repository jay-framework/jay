<html>
  <head>
    <script type="application/jay-data">
        data:
          contractPath:
          -     id: string
          bindItems:
          -     id: string
    </script>
  </head>
  <body>
    <div>
        <div forEach="contractPath" trackBy="id">
          <div>
            if you add ref on my div, it will:
            {id}
          </div>
        </div>
        <div forEach="bindItems" trackBy="id">
          <div ref="bli">
            {id}
          </div>
        </div>
    </div>
  </body>
</html>
